{
  "version": 2,
  "builds": [
    {
      "src": "backend_flask/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend_nextjs/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend_flask/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend_nextjs"
    }
  ]
}